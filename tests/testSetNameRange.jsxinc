/**
 * Setup
 */
Indigo.Tests.testSetNameRange = function() {};
Indigo.Tests.testSetNameRange.prototype = new Indigo.Tests.testSuite(app);
Indigo.Tests.testSetNameRange.prototype.constructor = Indigo.Tests.testSetNameRange;
Indigo.Tests.testSetNameRange.prototype.name = 'testSetNameRange';

Indigo.Tests.testSetNameRange.prototype.setUp = function () {
	this.pocient = new Indigo.BaseImposer(this.app);
	this.pocient.setup(this.getMockJob());
	this.pocient.getLabels();
};

/**
 * Test
 */
Indigo.Tests.testSetNameRange.prototype.run = function() {
	// Последовательный диапазон
	var longRange = this.pocient.setNameRange();
	var resultLong = this.assertEq(longRange, '001_002');
	this.log(resultLong, '.ManyLabels');
	// Повторяющийся диапазон
	this.pocient.labels.push(this.pocient.labels[0]);
	var dupRange = this.pocient.setNameRange();
	var resultDup = this.assertEq(dupRange, '001_002');
	this.log(resultDup, '.DuplicateLabels');
	// Одна этикетка
	this.pocient.labels = [ this.pocient.labels[0] ];
	var oneRange = this.pocient.setNameRange();
	var resultOne = this.assertEq(oneRange, '001');
	this.log(resultOne, '.OneLabel');
};

Indigo.Tests.testSetNameRange.prototype.tearDown = function() { };
