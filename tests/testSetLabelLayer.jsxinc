/**
 * Setup
 */
Indigo.Tests.testSetLabelLayer = function() {};
Indigo.Tests.testSetLabelLayer.prototype = new Indigo.Tests.testSuite(app);
Indigo.Tests.testSetLabelLayer.prototype.constructor = Indigo.Tests.testSetLabelLayer;
Indigo.Tests.testSetLabelLayer.prototype.name = 'testSetLabelLayer';

Indigo.Tests.testSetLabelLayer.prototype.setUp = function () {
	this.pocient = new Indigo.BaseImposer(this.app);
	var job = {
		"template" : 'testSetLabelLayer',
		"separations" : 'noMatter',
		"roll" : 'noMatter',
		"print_list" : ['noMatter'],
	};
	this.pocient.setup(job);
	this.pocient.templateFolder = this.testsFolder + '/' + this.name;
	this.pocient.openTemplate();
};

/**
 * Test
 */
Indigo.Tests.testSetLabelLayer.prototype.run = function() {
	var layer = this.pocient.setLabelLayer();
	var resultIns = this.assertInstanceOf(layer, Layer);
	var resultStr = this.assertEq(layer.name, 'label');
	var resultInt = this.assertEq(layer.zOrderPosition,1);
	this.log(resultIns, '.Type');
	this.log(resultStr, '.Name');
	this.log(resultInt, '.Position');
};
