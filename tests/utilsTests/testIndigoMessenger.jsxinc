/**
 * 
 * @classdesc testIndigoMessenger
 * 
 * @constructor
 */

Indigo.Tests.testIndigoMessenger = function() {};
Indigo.Tests.testIndigoMessenger.prototype = new Indigo.Tests.testSuite(app);
Indigo.Tests.testIndigoMessenger.prototype.constructor = Indigo.Tests.testIndigoMessenger;
Indigo.Tests.testIndigoMessenger.prototype.name = 'testIndigoMessenger';

/**
 * Объект статитеский, сетапа не будет
 */

/**
 * Test
 */
Indigo.Tests.testIndigoMessenger.prototype.run = function() {
	// Emit method test
	var message = {
		type: "question",
		question: "main",
	};
	var expected = {
		answer: 42,
	};
	var reply = Indigo.Messenger.emit(message);
	var resultEmit = this.assertObjectsSame(reply,expected);
	this.log(resultEmit, '.Emit');
};

/**
 * TearDown
 */
Indigo.Tests.testIndigoMessenger.prototype.tearDown = function() {
	Indigo.Messenger.closeSocket();
};
