{
	"tags": {
		"allowUnknownTags": false 
	},
	"source": {
		"includePattern": ".+\\.js.*$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown"],
	"markdown": {
		"parser": "gfm"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		},
		"systemName"      : "Indigo",
		"footer"          : "",
		"navType"         : "vertical",
		"theme"           : "spacelab",
		"linenums"        : true,
		"collapseSymbols" : false,
		"inverseNav"      : true
	},
}
